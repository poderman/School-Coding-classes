void setup() {
  Serial.begin(9600);
  randomSeed(analogRead(0));
  
  //This is for the event LED's
  pinMode(14, OUTPUT);
  pinMode(15, OUTPUT);
  pinMode(16, OUTPUT);
  pinMode(17, OUTPUT);
  pinMode(18, OUTPUT);
  pinMode(19, OUTPUT);
  pinMode(20, OUTPUT);
  pinMode(21, OUTPUT);

  //This is for the computer LED's
  pinMode(7, OUTPUT);
  pinMode(8, OUTPUT);
  pinMode(9, OUTPUT);
  pinMode(10, OUTPUT);
  
  
  // set up the LCD's number of columns and rows:
  lcd.begin(16, 2);
  
  lcd.print("Hello World!");
  delay(2000);
  lcd.clear();
  delay(1000);


